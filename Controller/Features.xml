<Features>

   <Representation location="Model.ChromosomalRepresentation.">
      <Technique name="Binary Representation" class="BinaryRepresentation"/>
      <Technique name="Float Point Representation" class="FloatPointRepresentation"/>
   </Representation>

   <Fitness location="Model.Fitness.">
      <Technique name="Proportional Fitness" class="ProportionalFitness"/>
      <Technique name="Linear Scaling Fitness" class="LinearScalingFitness"/>
      <Technique name="Linear Ranking Fitness" class="LinearRankingFitness"/>
      <Technique name="Non-Linear Ranking Fitness" class="NonLinearRankingFitness"/> 
   </Fitness>

   <SharingFunction location="Model.SharingFunction.">
      <Technique name="Binary Hamming Distance" class="GenotypicSimilarity.BinaryHammingDistance"/>
      <Technique name="Float Point Hamming Distance" class="GenotypicSimilarity.FloatPointHammingDistance"/>
      <Technique name="Euclidean Distance" class="PhenotypicSimilarity.EuclideanDistance"/>
   </SharingFunction>

   <Selection location="Model.Operator.Selection.">
      <Technique name="Roulette" class="Roulette"/>
      <Technique name="Stochastic Universal Sampling" class="StochasticUniversalSampling"/>
      <Technique name="Tournament" class="Tournament"/>
   </Selection>

   <Crossover location="Model.Operator.Crossover.">
      <Technique name="N-Points Crossover" class="NPointsCrossover"/>
      <Technique name="Uniform Crossover" class="UniformCrossover"/>
      <Technique name="Float Point Crossover" class="FloatPointCrossover"/>
   </Crossover>

   <Mutation location="Model.Operator.Mutation.">
      <Technique name="Binary Mutation" class="BinaryMutation"/>
      <Technique name="Float Point Mutation" class="FloatPointMutation"/>
   </Mutation>

   <MOEA location="Model.MOEA.">
      <Technique name="V.E.G.A. (Vector Evaluated Genetic Algorithm)" class="VEGA"/>
      <Technique name="M.O.G.A. (Multi Objective Genetic Algorithm)" class="MOGA"/>
      <Technique name="N.S.G.A. II (Nondominated Sorting Genetic Algorithm)" class="NSGAII"/>
      <Technique name="S.P.E.A. II (Strength Pareto Evolutionary Algorithm)" class="SPEAII"/>
   </MOEA>

</Features>
