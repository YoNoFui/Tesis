   </Representation>
   <Fitness location="Model.Fitness.">
      <Technique name="Proportional Fitness" class="ProportionalFitness" method="assign_proportional_fitness" />
      <Technique name="Linear Scaling Fitness" class="LinearScalingFitness" method="assing_linear_scaling_fitness"/>
      <Technique name="Linear Ranking Fitness" class="LinearRankingFitness" method="assign_linear_ranking_fitness"/>
